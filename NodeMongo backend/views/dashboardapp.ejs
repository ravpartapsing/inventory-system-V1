      <script>
         function addnew(){
              var appName=$("#newAppName").val();
              if(appName.length>0){
                window.location.href="http://localhost:3000/index?appName="+appName;
              }else{
                  alert("App Name is required")
              }
          }
          function openapp(id){
            window.location.href="http://localhost:3000/index?id="+id;
          }
          $(document).ready(function(){
              $.ajax({
                  url:"/app/getApps?asdf",
                  success:function(data){
                      console.log(data)
                      data.data.forEach(element => {
                          $("#mainwrapperss").append(`<div class="col-md-3 p-0 mt-1"> <div class="card m-2"  style="width: 18rem; border-radius: 0px; background: #353640; border-color:#353640" onclick='openapp("`+element.id+`")')>
